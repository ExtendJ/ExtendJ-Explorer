<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<MenuBar styleClass="typeListView" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jastaddad.ui.controllers.TopMenuController">
    <Menu text="File" fx:id="topMenuFileMenu">
        <items>
            <Menu text="Export" fx:id="topMenuExportMenu">
            </Menu>
            <SeparatorMenuItem/>
            <MenuItem text="Exit" fx:id="exitMenuItem">
            </MenuItem>
        </items>
    </Menu>
    <Menu text="View">
        <items>
            <MenuItem text="F" fx:id="toggleMinimizeMenuItem">
                <graphic>
                    <Label text="Toggle minimize all"></Label>
                </graphic>
            </MenuItem>
        </items>
    </Menu>
</MenuBar>