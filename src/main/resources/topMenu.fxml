<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<MenuBar styleClass="typeListView" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jastaddad.ui.controllers.TopMenuController">
    <Menu text="File" fx:id="topMenuFileMenu">
        <items>
            <Menu text="Export" fx:id="topMenuExportMenu">
            </Menu>
            <SeparatorMenuItem/>
            <MenuItem text="Exit" fx:id="exitMenuItem">
            </MenuItem>
        </items>
    </Menu>
    <Menu text="View">
        <items>
            <MenuItem text="" fx:id="toggleMinimizeMenuItem">
                <graphic>
                    <HBox>
                        <Label styleClass="MenuItemText" text="Toggle minimize all"></Label>
                        <Label></Label>
                        <Label styleClass="MenuItemShort" text="F"></Label>
                    </HBox>
                </graphic>
            </MenuItem>
        </items>
    </Menu>
</MenuBar>