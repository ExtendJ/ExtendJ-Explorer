configs{
	use = include;
}
filter include{
	:DebuggerConfig;
	:Filter;
	:IdDecl{
		when{
			child of :Filter;
		}
	}
}

filter styles{
	:Value{
		style{
			node-color = #44ff00;
			node-shape = "circle";
		}

		show{
			:debuggerConfig;
		}
	}
	:Bool{
		style{
			node-color = #44ffff;
			node-shape= "rectangle";
		}
	}
}
